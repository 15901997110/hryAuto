<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.haier.mapper.UserMapper">
 <select id="selectByUsername" parameterType="java.lang.String" resultType="com.haier.po.User">
     SELECT * FROM `user` WHERE `user`.`identity`=#{username}
 </select>
</mapper>